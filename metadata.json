{
  "name": "yuav-autofs",
  "version": "1.0.1",
  "author": "Jon Skarpeteig",
  "summary": "Installs and configures autofs",
  "license": "Apache-2.0",
  "tags": ["autofs", "automount"],
  "source": "https://github.com/Yuav/puppet-autofs",
  "project_page": "https://github.com/Yuav/puppet-autofs",
  "issues_url": "https://github.com/Yuav/puppet-autofs",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
